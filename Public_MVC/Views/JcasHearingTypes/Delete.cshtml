@model FairfaxCounty.JCAS_Public_MVC.Models.JcasHearingType

@{
    ViewBag.Title = "Delete Hearing Type";
}

<h2>@ViewBag.Title</h2>

<h3>Are you sure you want to delete this Hearing Type?</h3>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Hearing Type Code
            </dt>

            <dd>
                @Html.DisplayFor(model => model.HearingTypeCode)
            </dd>

            <dt>
                Hearing Type Name
            </dt>

            <dd>
                @Html.DisplayFor(model => model.HearingTypeName)
            </dd>

        </dl>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Delete" class="btn btn-default" />
        </div>
    </div>
}
