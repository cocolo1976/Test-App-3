@model FairfaxCounty.JCAS_Public_MVC.Models.JcasCourtDateNote

@{
    ViewBag.Title = "Delete Court Date Note";
}

<h2>@ViewBag.Title</h2>

<h3>Are you sure you want to delete this Court Date Note?</h3>
@using (Html.BeginForm())
{
    <div class="form-horizontal">
        @Html.AntiForgeryToken()
        <div class="form-group">
            <hr />
            <dl>
                <dt class="col-md-2">
                    From Date
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.FromDate)
                </dd>

                <dt class="col-md-2">
                    To Date
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ToDate)
                </dd>

                <dt class="col-md-2">
                    Display Order
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.DisplayOrder)
                </dd>

                <dt class="col-md-2">
                    Note
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Note)
                </dd>
            </dl>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Delete" class="btn btn-default" />
            </div>
        </div>
    </div>
}
