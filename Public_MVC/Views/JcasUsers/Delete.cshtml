@model FairfaxCounty.JCAS_Public_MVC.Models.JcasUser

@{
    ViewBag.Title = "Delete Internal User";
}
<h2>@ViewBag.Title</h2>

<h3>Are you sure you want to delete this Internal User?</h3>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                User Login Id
            </dt>

            <dd>
                @Html.DisplayFor(model => model.UserLoginId)
            </dd>

            <dt>
                User Name
            </dt>

            <dd>
                @Html.DisplayFor(model => model.UserName)
            </dd>

            <dt>
                User Role
            </dt>

            <dd>
                @Html.DisplayFor(model => model.UserRole)
            </dd>
        </dl>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Delete" class="btn btn-default" />
        </div>
    </div>
}